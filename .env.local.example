NEXT_PUBLIC_SUPABASE_URL=your-supabase-url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key

# App URL (for OAuth callbacks and email links)
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Resend Email Configuration
# Get your API key from: https://resend.com/api-keys
RESEND_API_KEY=your-resend-api-key
RESEND_FROM_EMAIL=onboarding@yourdomain.com

# Square OAuth Configuration
# Get these from: https://developer.squareup.com/apps → Your App → Credentials
NEXT_PUBLIC_SQUARE_APPLICATION_ID=your-square-application-id
SQUARE_APPLICATION_SECRET=your-square-application-secret
SQUARE_ENVIRONMENT=sandbox

# OpenAI Configuration
# Get your API key from: https://platform.openai.com/api-keys
OPENAI_API_KEY=your-openai-api-key
